@model System.Collections.Hashtable
<!DOCTYPE html>
<html lang="en">
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=UTF-8"/>
	<title>GroupDocs Sample04</title>
    <link rel="stylesheet" type="text/css" href="/style.css" />

</head>
<body>
<h3 style="text-align:center;"><a href="/samples">GroupDocs .NET SDK Samples</a> - Sample04</h3>

<div class='samplecontent' style="padding:10px;">
   <span class="description"><i>This sample will show how to download a file from GroupDocs Storage using .NET SDK</i></span> <br/>
   <br />
   <span class="documentation"><a href="/docs/controllers/indexcontroller.html#section_50">Docs for this sample</a></span>
   <br />
   <br/><p>You entered:
@if (Model != null) {
 
           <p>ClientID = @Model["client_id"]</p>
           <p>PrivateKey = @Model["private_key"]</p>
           <p><font color="red">@Model["error"]</font></p>
      
}
   <br/>
   <div id="requestForm" style="padding:20px; border:1px solid black;">
   <p> Enter data for request and press "Make request" button </p>
   @using (Html.BeginForm("sample04", "Samples", FormMethod.Post, new { enctype = "multipart/form-data" }))
   {
        <label for='client_id'>GroupDocs Client ID</label>
        <br />
        <input type='text', name='client_id' value="" />
        <br />
        <label for='private_key'>GroupDocs Private Key</label>
        <br />
        <input type='text', name='private_key'  value="" />
        <br /> 
        <label for='private_key'>FileID</label>
       <br />
       <input type='text', name='fileId'  value="" />
        <br />
       <label for='server_type'>Base path<span class="optional">(Optional)</span></label>
        <br />
        <input type ="text" name="server_type" id="server_type" />
       <br />
        <input type='submit' value='Download'/>
   }
    
        @if (Model != null) {
            if (Model["error"] != null)
            {
                @Model["error"]
            } else {
                <font color="green">File was downloaded to the <font color="blue">@Model["path"]@Model["name"]</font> folder</font> <br />
            }
        }
  </div>
</div>

    @Html.Partial("links")
</body>
</html>