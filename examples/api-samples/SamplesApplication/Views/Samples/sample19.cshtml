@model System.Collections.Hashtable
<!DOCTYPE html>
<html lang="en">
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=UTF-8"/>
	<title>GroupDocs Sample19</title>
    <link rel="stylesheet" type="text/css" href="/style.css" />

</head>
<body>
<h3 style="text-align:center;"><a href="/samples">GroupDocs .NET SDK Samples</a> - Sample19</h3>

<div class='samplecontent' style="padding:10px;">
   <span class="description"><i>This sample will show how to Compare documents using .NET SDK</i></span> <br/>
   <br />
   <span class="documentation"><a href="/docs/controllers/indexcontroller.html#section_299">Docs for this sample</a></span>
   <br />
   <br/><p>You entered:
@if (Model != null) {
 
           <p>ClientID = @Model["client_id"]</p>
           <p>PrivateKey = @Model["private_key"]</p>
           <p>Source file Id = @Model["sourceFileId"]</p>
           <p>target file id = @Model["targetFileId"]</p>
           <p>Callback = @Model["callback"]</p>
           <p><font color="red">@Model["error"]</font></p>
      
}
   <br/>
   <div id="requestForm" style="padding:20px; border:1px solid black;">
   <p> Enter data for request and press "Make request" button </p>
   @using (Html.BeginForm("sample19", "Samples", FormMethod.Post, new { enctype = "multipart/form-data" }))
   {
        <label for='client_id'>GroupDocs Client ID</label>
        <br />
        <input type='text', name='client_id' value="" />
        <br />
        <label for='private_key'>GroupDocs Private Key</label>
        <br />
        <input type='text', name='private_key'  value="" />
        <br /> 
        <label for='sourceFileId'>sourceFileId</label>
            <br />
            <input type='text', name='sourceFileId'  value="" />
            <br />
            <label for='targetFileId'>targetFileId</label>
            <br />
            <input type='text', name='targetFileId'  value="" />
            <br />
            <label for='callbackUrl'>callbackUrl</label>
            <br />
            <input type='text', name='callbackUrl'  value="" />
            <br />
            <label for='server_type'>Choose Server</label>
            <br />
            <select name="server_type" id="server_type">
                <option selected value="https://api.groupdocs.com/v2.0">Prod</option>
                <option value="https://dev-api.groupdocs.com/v2.0">Dev</option>
                <option value="https://stage-api.groupdocs.com/v2.0">Stage</option>
            </select>
            <br />
        <input type='submit' value='Make Request'/>
   }
       
        @if (Model != null) {
            if (Model["error"] != null)
            {
                @Model["error"]
            } else {
                    <div  style="padding:20px; border:1px solid black;">
                        <p> Results: </p>
                        @{
                            if(Model["basePath"].Equals("https://api.groupdocs.com/v2.0")) {
                                <iframe src = "https://apps.groupdocs.com/document-viewer/embed/@Model["guid"]" frameborder="0" width="500" height="650"></iframe>
                            } else if(Model["basePath"].Equals("https://dev-api.groupdocs.com/v2.0")) {
                                <iframe src = "https://dev-apps.groupdocs.com/document-viewer/embed/@Model["guid"]" frameborder="0" width="500" height="650"></iframe>
                            } else if(Model["basePath"].Equals("https://stage-api.groupdocs.com/v2.0")) {
                                <iframe src = "https://stage-apps.groupdocs.com/document-viewer/embed/@Model["guid"]" frameborder="0" width="500" height="650"></iframe>
                            }
                         }
                   </div>
            }
        }
  </div>
</div>

    @Html.Partial("links")
</body>
</html>