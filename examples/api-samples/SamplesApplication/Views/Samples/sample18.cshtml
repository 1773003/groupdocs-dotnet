@model System.Collections.Hashtable
<!DOCTYPE html>
<html lang="en">
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=UTF-8"/>
	<title>GroupDocs Sample18</title>
    <link rel="stylesheet" type="text/css" href="/style.css" />

</head>
<body>
<h3 style="text-align:center;"><a href="/samples">GroupDocs .NET SDK Samples</a> - Sample18</h3>

<div class='samplecontent' style="padding:10px;">
   <i>This sample will show how to convert Doc to Docx, Docx to Doc, Docx and DOC to PDF and PPT to PDF, HTML to DOC and DOCX using .NET SDK</i> <br/>
   <br />
   <a href="/docs/controllers/indexcontroller.html#section_176">Docs for this sample</a>
   <br />
   <br/><p>You entered:
@if (Model != null) {
 
           <p>ClientID = @Model["client_id"]</p>
           <p>PrivateKey = @Model["private_key"]</p>
           <p>File Id = @Model["fileId"]</p>
           <p>Convert type = @Model["type"]</p>
           <p><font color="red">@Model["error"]</font></p>
      
}
   <br/>
   <div id="requestForm" style="padding:20px; border:1px solid black;">
   <p> Enter data for request and press "Make request" button </p>
   @using (Html.BeginForm("sample18", "Samples", FormMethod.Post, new { enctype = "multipart/form-data" }))
   {
        <label for='client_id'>GroupDocs Client ID</label>
        <br />
        <input type='text', name='client_id' value="" />
        <br />
        <label for='private_key'>GroupDocs Private Key</label>
        <br />
        <input type='text', name='private_key'  value="" />
        <br /> 
        <label for='private_key'>FileID</label>
        <br />
        <input type='text', name='fileId'  value="" />
        <br />
        <label for='private_key'>Callback URL</label>
        <br />
        <input type='text', name='callback'  value="" />
        <br />
        <select name="convert_type" id="convert_type">
                            <option value="doc">Doc</option>
                            <option value="pdf">PDF</option>
                            <option value="docx">Docx</option>
                            <option value="ppt">PPT</option>
        </select>
        <br />
        <input type='submit' value='Make Request'/>
   }
        <br />
        <a href="http://groupdocs-php-samples.herokuapp.com/templates/testdocument.html">Download sample HTML file 1</a> 
       <br />
        <a href="http://groupdocs-php-samples.herokuapp.com/templates/base-simple_source.html">Download sample HTML file 2</a>
    
        @if (Model != null) {
            if (Model["error"] != null)
            {
                @Model["error"]
            } else {
                    <div  style="padding:20px; border:1px solid black;">
                        <p> Results: </p>
                        <iframe src="https://apps.groupdocs.com/document-viewer/embed/@Model["guid"]" frameborder="0" width="100%" height="600"></iframe>
                   </div>
            }
        }
  </div>
</div>

    @Html.Partial("links")
</body>
</html>