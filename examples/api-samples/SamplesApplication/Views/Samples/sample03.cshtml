@model System.Collections.Hashtable
<!DOCTYPE html>
<html lang="en">
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=UTF-8"/>
	<title>GroupDocs Sample03</title>
    <link rel="stylesheet" type="text/css" href="/style.css" />

</head>
<body>
<h3 style="text-align:center;"><a href="/samples">GroupDocs .NET SDK Samples</a> - Sample03</h3>

<div class='samplecontent' style="padding:10px;">
   <i>This sample will show how to upload a file to GroupDocs using .NET SDK</i> <br/>
   <br />
   <a href="/docs/controllers/indexcontroller.html#section_35">Docs for this sample</a>
   <br />
   <br/><p>You entered:
@if (Model != null) {
 
           <p>ClientID = @Model["client_id"]</p>
           <p>PrivateKey = @Model["private_key"]</p>
           <p>Base path = @Model["basePath"]</p>
           <p><font color="red">@Model["error"]</font></p>
           
}
   <br/>
   <div id="requestForm" style="padding:20px; border:1px solid black;">
   <p> Enter data for request and press "Make request" button </p>
   @using (Html.BeginForm("sample03", "Samples", FormMethod.Post, new { enctype = "multipart/form-data" }))
   {
        <label for='client_id'>GroupDocs Client ID</label>
        <br />
        <input type='text', name='client_id' value="" />
        <br />
        <label for='private_key'>GroupDocs Private Key</label>
        <br />
        <input type='text', name='private_key'  value="" />
        <br /> 
        <label for='file'>File</label>
        <br />
        <input type='file' name='file'  />
        <br />
       <br />
        <p><font color="green">Or you can enter URL from where to upload file</font></p>
        <br />
        <label for='private_key'>File URL</label>
        <br />
        <input type='text', name='url'  value="" />
        <br />
        <label for='server_type'>Base Path</label>
         <br />
        <input name="server_type" id="server_type" />
        <br />
        <input type='submit' value='Make Request'/>
   }
    </div>
@if (Model != null) {
    
    <div  style="padding:20px; border:1px solid black;">
        <p>Results:</p>
        @if (Model["error"] != null) {
            @Model["error"]
        } else {
            
             if(Model["basePath"].Equals("")) {
                <iframe src = "https://apps.groupdocs.com/document-viewer/embed/@Model["guid"]" frameborder="0" width="700" height="700"></iframe>
            }else if(Model["basePath"].Equals("https://api.groupdocs.com/v2.0")) {
                <iframe src = "https://apps.groupdocs.com/document-viewer/embed/@Model["guid"]" frameborder="0" width="700" height="700"></iframe>
            } else if(Model["basePath"].Equals("https://dev-api.groupdocs.com/v2.0")) {
                <iframe src = "https://dev-apps.groupdocs.com/document-viewer/embed/@Model["guid"]" frameborder="0" width="700" height="700"></iframe>
            } else if(Model["basePath"].Equals("https://stage-api.groupdocs.com/v2.0")) {
                <iframe src = "https://stage-apps.groupdocs.com/document-viewer/embed/@Model["guid"]" frameborder="0" width="700" height="700"></iframe>
            } else if(Model["basePath"].Equals("https://realtime-api.groupdocs.com/v2.0")) {
                <iframe src = "https://realtime-apps.groupdocs.com/document-viewer/embed/@Model["guid"]" frameborder="0" width="700" height="700"></iframe>
            }
                       
        }
    </div>
}
</div>

    @Html.Partial("links")
</body>
</html>