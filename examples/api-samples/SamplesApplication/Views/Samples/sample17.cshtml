@model System.Collections.Hashtable
<!DOCTYPE html>
<html lang="en">
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=UTF-8"/>
	<title>GroupDocs Sample17</title>
    <link rel="stylesheet" type="text/css" href="/style.css" />

</head>
<body>
<h3 style="text-align:center;"><a href="/samples">GroupDocs .NET SDK Samples</a> - Sample17</h3>

<div class='samplecontent' style="padding:10px;">
   <span class="description"><i>This sample will show how to upload a file into the storage and compress it into zip archive using .NET SDK</i></span> <br/>
   <br />
   <span class="documentation"><a href="/docs/controllers/indexcontroller.html#section_265">Docs for this sample</a></span>
   <br />
   <br/><p>You entered:
@if (Model != null) {
 
           <p>ClientID = @Model["client_id"]</p>
           <p>PrivateKey = @Model["private_key"]</p>
           <p><font color="red">@Model["error"]</font></p>
      
}
   <br/>
   <div id="requestForm" style="padding:20px; border:1px solid black;">
   <p> Enter data for request and press "Make request" button </p>
   @using (Html.BeginForm("sample17", "Samples", FormMethod.Post, new { enctype = "multipart/form-data" }))
   {
        <label for='client_id'>GroupDocs Client ID</label>
        <br />
        <input type='text', name='client_id' value="" />
        <br />
        <label for='private_key'>GroupDocs Private Key</label>
        <br />
        <input type='text', name='private_key'  value="" />
        <br /> 
        <label for='server_type'>Base Path <font color="grey">(Optional)</font></label>
         <br />
        <input name="server_type" id="server_type" />
        <br />
        <input type="radio" name="sourse" value="local" id="localField" onClick="display('local');" checked>Local file </input>
                                                                         <br />
        <input type="radio" name="sourse" value="url" id="urlField" onClick="display('url');" >From URL </input>
                                                                     <br />
         
        <br />
        <label for='file' id="file">File</label>
        <br />
        <input type='file' name='file' id="filefield" />
        
        <label for='private_key' style="display:none;" id="url">File URL</label>
        <br />
        <input type='text', name='url'  value="" id="urlfield" style="display:none;" />
        <br />
        <input type='submit' value='Upload'/>
   }
    </div>
@if (Model != null) {
    
    <div  style="padding:20px; border:1px solid black;">
        <p>Results:</p>
        @if (Model["error"] != null) {
            @Model["error"]
        } else {
          
            <p><font color="green">@Model["message"]</font></p>
        }
    </div>
}
</div>

    @Html.Partial("links")
    <script type="text/javascript">
        function display(_element_id) {

            if (_element_id == "local") {
                var element1 = document.getElementById("file");
                var element2 = document.getElementById("filefield");
                var element3 = document.getElementById("url");
                var element4 = document.getElementById("urlfield");
                element1.style.display = "inline";
                element2.style.display = "inline";
                element3.style.display = "none";
                element4.style.display = "none";

            }

            if (_element_id == "url") {
                var element1 = document.getElementById("url");
                var element2 = document.getElementById("urlfield");
                var element3 = document.getElementById("file");
                var element4 = document.getElementById("filefield");
                element1.style.display = "inline";
                element2.style.display = "inline";
                element3.style.display = "none";
                element4.style.display = "none";
            }

        }
</script>
</body>
</html>