@model int[]
 

<!DOCTYPE html>
<html lang="en">
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=UTF-8"/>
	<title>GroupDocs Sample01</title>
    <link rel="stylesheet" type="text/css" href="/style.css" />

</head>
<body>
<h3 style="text-align:center;"><a href="/index.php">GroupDocs .NET SDK Samples</a> - Sample1</h3>

<div class='samplecontent' style="padding:10px;">
   <i>This sample will show how to use Signer object to be authorized at GroupDocs and how to get GroupDocs user infromation using PHP SDK</i> <br/>
   <br />
   <a href="docs/sample01.html">Docs for this sample</a>
   <br />
   <br/><p>You entered: 
   <p>ClientID = {{userId}}
   <p>PrivateKey = {{privateKey}}
   <p><font color="red">{{error}}</font>
   <div  style="padding:20px; border:1px solid black;">
        <p> Results: </p>
        <table style="border:1px double black;">
          <tr>
              <th>Field</th><th>Value</th>
          </tr>
          <tr>
               <td>First Name</td><td>{{userInfo->firstname}}</td>
          </tr>
          <tr>
               <td>Last Name</td><td>{{userInfo->lastname}}</td>
          </tr>
          <tr>
               <td>Nick Name</td><td>{{userInfo->nickname}}</td>
          </tr>
          <tr>
               <td>Primary Email</td><td>{{userInfo->primary_email}}</td>
          </tr>
        </table>
        
   </div>
   <br/>
   <div id="requestForm" style="padding:20px; border:1px solid black;">
   <p> Enter data for request and press "Make request" button </p>
   @using (Html.BeginForm("sample01", "Samples", FormMethod.Post, new { enctype = "multipart/form-data" }))
   {
        <label for='client_id'>GroupDocs Client ID</label>
        <br />
        <input type='text', name='client_id' value="@Model[0]" />
        <br />
        <label for='private_key'>GroupDocs Private Key</label>
        <br />
        <input type='text', name='private_key'  value="privateKey" />
        <br />
        <input type='submit' value='Make Request'/>
   }
    </div>
</div>

    @Html.Partial("links")
</body>
</html>