@model System.Collections.Hashtable
<!DOCTYPE html>
<html lang="en">
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=UTF-8"/>
	<title>GroupDocs Sample17</title>
    <link rel="stylesheet" type="text/css" href="/style.css" />

</head>
<body>
<h3 style="text-align:center;"><a href="/samples">GroupDocs .NET SDK Samples</a> - Sample17</h3>

<div class='samplecontent' style="padding:10px;">
   <i>This sample will show how to upload a file into the storage and compress it into zip archive using .NET SDK</i> <br/>
   <br />
   <a href="/docs/controllers/indexcontroller.html#section_35">Docs for this sample</a>
   <br />
   <br/><p>You entered:
@if (Model != null) {
 
           <p>ClientID = @Model["client_id"]</p>
           <p>PrivateKey = @Model["private_key"]</p>
           <p><font color="red">@Model["error"]</font></p>
      
}
   <br/>
   <div id="requestForm" style="padding:20px; border:1px solid black;">
   <p> Enter data for request and press "Make request" button </p>
   @using (Html.BeginForm("sample17", "Samples", FormMethod.Post, new { enctype = "multipart/form-data" }))
   {
        <label for='client_id'>GroupDocs Client ID</label>
        <br />
        <input type='text', name='client_id' value="" />
        <br />
        <label for='private_key'>GroupDocs Private Key</label>
        <br />
        <input type='text', name='private_key'  value="" />
        <br /> 
        <label for='file'>File</label>
        <br />
        <input type='file' name='file'  />
        <br />
        <input type='submit' value='Upload'/>
   }
    </div>
@if (Model != null) {
    
    <div  style="padding:20px; border:1px solid black;">
        <p>Results:</p>
        @if (Model["error"] != null) {
            @Model["error"]
        } else {
           <p>Results:</p>
            <font color="green"><p>@Model["message"]</p></font>
        }
    </div>
}
</div>

    @Html.Partial("links")
</body>
</html>