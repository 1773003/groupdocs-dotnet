@model System.Collections.Hashtable
<!DOCTYPE html>
<html lang="en">
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=UTF-8"/>
	<title>GroupDocs Sample28</title>
    <link rel="stylesheet" type="text/css" href="/style.css" />

</head>
<body>
<h3 style="text-align:center;"><a href="/samples">GroupDocs .NET SDK Samples</a> - Sample28</h3>

<div class='samplecontent' style="padding:10px;">
   <span class="description"><i>This sample will show how to delete all annotations Document using .NET SDK</i></span> <br/>
   <br />
   <span class="documentation"><a href="/docs/controllers/indexcontroller.html#section_659">Docs for this sample</a></span>
   <br />
   <br/><p>The data you provided </p>
@if (Model != null)
{
 
           <p>ClientID = @Model["client_id"]</p>
           <p>PrivateKey = @Model["private_key"]</p>
           <p>FileId = @Model["fileId"]</p>
           <p><font color="red">@Model["error"]</font></p>
      
}
   <br/>
   <div id="requestForm" style="padding:20px; border:1px solid black;">
   <p> Enter data for request and press "Delete All Annotations" button </p>
   @using (Html.BeginForm("sample28", "Samples", FormMethod.Post, new { enctype = "multipart/form-data" }))
   {
        <label for='client_id'>GroupDocs Client ID</label>
        <br />
        <input type='text' id='client_id' name='client_id' value="" />
        <br />
        <label for='private_key'>GroupDocs Private Key</label>
        <br />
        <input type='text' id='private_key' name='private_key'  value="" />
        <br />
        <label for='fileId'>FileID</label>
        <br />
        <input type='text' name='fileId'  value="" />
        <br />
        <label for='server_type'>Base path<span class="optional">(Optional)</span></label>
        <br />
        <input type ="text" name="server_type" id="server_type" />
        <br />
        <input type='submit' value='Delete All Annotations'/>
   }
    
        @if (Model != null)
        {
            if (Model["error"] != null)
            {
                @Model["error"]
            }
            else
            {
                  <div style="padding:20px; border:1px solid black;">
                       <span style="color: red">@Model["message"]</span>
                      <iframe width="800" height="650" src="@Model["url"]"></iframe>
                  </div>
            }
        }
  </div>
</div>

    @Html.Partial("links")
   
</body>
</html>